# Example environment configuration for pretalx-docker
# Copy this file to .env and fill in your actual values

# Zrok environment name - identifier for your zrok environment
ZROK_ENV_NAME=pretalx@coltrane

# Zrok service name - unique name for your zrok service
ZROK_NAME=ref11

# Zrok API endpoint - URL of your zrok API server
ZROK_API_ENDPOINT=https://zrok.zrok.lcas.group

# Zrok authentication token - get this from your zrok dashboard
ZROK_TOKEN=your-zrok-token-here

# Basic authentication credentials for zrok service (format: username:password)
ZROK_BASICAUTH=uoa11:password
